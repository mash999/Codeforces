#include <iostream>

using namespace std;

int main()
{
    int i,sum=0,in;
    for(i=0;i<5;i++){
        cin>>in;
        sum = sum+in;
    }

    if(sum==0){
        cout<<"-1";
    }

    else if(sum%5==0){
        i = sum/5;
        cout<<i;
    }

    else{
        cout<<"-1";
    }
    return 0;
}
