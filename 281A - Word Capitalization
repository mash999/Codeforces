#include <iostream>

using namespace std;

int main()
{
    int i,l;
    string s;
    cin>>s;
    l = s.length();
    char c = s.at(0);
    for(i=0;i<l;i++){
        if(i==0){
            c = toupper(c);
            cout<<c;
        }
        else{
            cout<<s.at(i);
        }
    }
    return 0;
}
